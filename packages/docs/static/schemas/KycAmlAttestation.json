{
  "$ref": "#/definitions/KYCAMLAttestation",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "KYCAMLAttestation": {
      "additionalProperties": false,
      "properties": {
        "@type": {
          "const": "KYCAMLAttestation",
          "type": "string"
        },
        "approvalDate": {
          "type": "string"
        },
        "authorityCallbackUrl": {
          "type": "string"
        },
        "authorityId": {
          "type": "string"
        },
        "authorityName": {
          "type": "string"
        },
        "authorityUrl": {
          "type": "string"
        },
        "expirationDate": {
          "type": "string"
        },
        "serviceProviders": {
          "items": {
            "$ref": "#/definitions/KYCAMLProvider"
          },
          "type": "array"
        }
      },
      "required": ["@type", "authorityId", "approvalDate"],
      "type": "object"
    },
    "KYCAMLProvider": {
      "additionalProperties": false,
      "properties": {
        "@type": {
          "const": "KYCAMLProvider",
          "type": "string"
        },
        "comment": {
          "type": "string"
        },
        "completionDate": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "score": {
          "type": "number"
        }
      },
      "required": ["@type"],
      "type": "object"
    }
  }
}
